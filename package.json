{
    "name": "gigleer-web",
    "description": "gigleer web",
    "version": "1.1.3",
    "author": "",
    "dependencies": {
        "@types/react": "16.9.49",
        "@types/react-dom": "16.9.8",
        "@types/styled-components": "5.1.3",
        "@typescript-eslint/eslint-plugin": "3.5.0",
        "@typescript-eslint/parser": "4.3.0",
        "bulma": "^0.9.1",
        "eslint": "7.10.0",
        "eslint-config-prettier": "6.12.0",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.21.2",
        "eslint-plugin-react-hooks": "4.1.2",
        "gatsby": "^2.24.66",
        "gatsby-image": "^2.4.20",
        "gatsby-plugin-netlify": "^2.3.16",
        "gatsby-plugin-netlify-cms": "4.3.15",
        "gatsby-plugin-purgecss": "^5.0.0",
        "gatsby-plugin-react-helmet": "^3.3.12",
        "gatsby-plugin-sass": "2.3.14",
        "gatsby-plugin-sharp": "^2.6.37",
        "gatsby-plugin-typescript": "^2.4.21",
        "gatsby-plugin-web-font-loader": "^1.0.4",
        "gatsby-remark-copy-linked-files": "^2.3.17",
        "gatsby-remark-images": "^3.3.31",
        "gatsby-remark-relative-images": "^2.0.2",
        "gatsby-source-filesystem": "^2.3.31",
        "gatsby-transformer-remark": "^2.8.36",
        "gatsby-transformer-sharp": "^2.5.16",
        "lodash": "^4.17.20",
        "lodash-webpack-plugin": "^0.11.4",
        "netlify-cms-app": "^2.12.25",
        "netlify-cms-media-library-cloudinary": "^1.3.2",
        "netlify-cms-media-library-uploadcare": "^0.5.2",
        "node-sass": "4.14.1",
        "prettier": "2.1.2",
        "prop-types": "^15.6.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^5.2.0",
        "react-mobile-store-button": "^0.0.6",
        "react-router-dom": "5.2.0",
        "styled-components": "^5.0.0",
        "stylelint": "^13.0.0",
        "stylelint-config-recess-order": "^2.0.3",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1"
    },
    "license": "MIT",
    "scripts": {
        "clean": "gatsby clean",
        "start": "npm run develop",
        "build": "npm run clean && gatsby build",
        "develop": "npm run clean && gatsby develop",
        "typecheck": "tsc --project tsconfig.json #",
        "lint:js": "eslint ./*.js --cache",
        "lint:json": "prettier '*.json'",
        "lint:ts": "eslint ./src/**/*.ts ./src/**/*.tsx  --cache",
        "lint:style": "stylelint './src/**/*.tsx' --syntax css-in-js",
        "lint:fix:ts": "yarn lint:ts --fix",
        "lint:fix:js": "yarn lint:js --fix",
        "lint:fix:json": "yarn lint:json --write",
        "lint:fix:style": "yarn lint:style --fix",
        "lint": "run-p -l -n -c --aggregate-output lint:*",
        "lint:fix": "run-p -l -n -c --aggregate-output lint:fix:*",
        "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.161",
        "@types/react-helmet": "^6.1.0",
        "babel-plugin-styled-components": "^1.11.1",
        "babel-preset-gatsby": "^0.5.11",
        "gatsby-plugin-graphql-codegen": "^2.7.1",
        "gatsby-plugin-styled-components": "^3.3.12",
        "gatsby-plugin-tslint": "^0.0.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "tslint": "^6.1.3",
        "tslint-loader": "^3.5.4",
        "typescript": "^4.0.3"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "yarn lint:fix:js",
            "git add"
        ],
        "*.{ts,tsx}": [
            "yarn typecheck",
            "yarn lint:fix:ts",
            "yarn lint:style",
            "git add"
        ],
        "*.{json}": [
            "yarn lint:fix:json",
            "git add"
        ]
    }
}
